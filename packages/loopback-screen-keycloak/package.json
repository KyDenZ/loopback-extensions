{
  "name": "@shrinedev/loopback-screen-keycloak",
  "version": "0.0.1",
  "description": "Keycloak LoopBack Controller Screen. ",
  "keywords": [
    "keycloak",
    "loopback",
    "typescript",
    "controller",
    "screen"
  ],
  "author": "Douglas Drouillard <drouillard@gmail.com>",
  "homepage": "https://www.shrinedev.com",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "clean": "lb-clean loopback-screen-keycloak*.tgz dist package api-docs",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/unit/**/*.js\" \"dist/__tests__/integration/**/*.js\" \"dist/__tests__/acceptance/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-screen-keycloak*.tgz && tree package && npm run clean"
  },
  "dependencies": {
    "@loopback/rest": "^1.7.0",
    "@shrinedev/loopback-screen": "0.0.1",
    "@shrinedev/middleware-runner": "0.0.1",
    "cookie": "0.3.1",
    "keycloak-connect": "4.8.3"
  },
  "devDependencies": {
    "@loopback/build": "1.3.1",
    "@loopback/testlab": "1.0.7",
    "@loopback/tslint-config": "2.0.1",
    "@types/cookie": "0.3.2",
    "@types/keycloak-connect": "^4.5.1"
  }
}
